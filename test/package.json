    "type": "git",
    "url": "git+https://github.com/alecmestroni/cypress-aws-secrets-manager.git"
  },
  "keywords": [
    "aws",
    "cypress",
    "secret",
    "secretsmanager",
    "aws-secrets-manager",
    "env"
  ],
