  "dependencies": {
    "cypress": "^13.3.2",
    "cypress-aws-secrets-manager": "file:..",
    "cypress-env": "^1.1.2",
    "jest": "^29.7.0",
    "@aws-sdk/client-secrets-manager": "^3.637.0",
